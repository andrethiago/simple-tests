CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE SEQUENCE CONFERENCIA_SEQ AS INTEGER START WITH 15 
CREATE SEQUENCE PALESTRA_SEQ AS INTEGER START WITH 23 
CREATE SEQUENCE PALESTRANTE_SEQ AS INTEGER START WITH 17 
CREATE MEMORY TABLE CONFERENCIA(ID_CONFERENCIA INTEGER NOT NULL PRIMARY KEY,NOME VARCHAR(200) NOT NULL,DESCRICAO VARCHAR(500) NOT NULL,DATA_INICIO DATE NOT NULL,DATA_FIM DATE NOT NULL,ENDERECO VARCHAR(1000),CEP VARCHAR(9))
CREATE MEMORY TABLE PALESTRANTE(ID_PALESTRANTE INTEGER NOT NULL PRIMARY KEY,NOME VARCHAR(500) NOT NULL,RESUMO VARCHAR(2000))
CREATE MEMORY TABLE PALESTRA(ID_PALESTRA INTEGER NOT NULL PRIMARY KEY,NOME VARCHAR(200) NOT NULL,RESUMO VARCHAR(2000),DATA TIMESTAMP NOT NULL,ID_CONFERENCIA INTEGER NOT NULL,ID_PALESTRANTE INTEGER NOT NULL,CONSTRAINT FK_CONFERENCIA FOREIGN KEY(ID_CONFERENCIA) REFERENCES CONFERENCIA(ID_CONFERENCIA) ON DELETE CASCADE,CONSTRAINT FK_PALESTRANTE FOREIGN KEY(ID_PALESTRANTE) REFERENCES PALESTRANTE(ID_PALESTRANTE) ON DELETE CASCADE)
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 10
SET SCHEMA PUBLIC
